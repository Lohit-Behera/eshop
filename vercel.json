{
    "builds": [{
        "src": "./backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
        "src": "./frontend",
        "use": "@vercel/static-build",
        "config": {
          "outputDirectory": "dist/"
        }
      }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/wsgi.py"
        }
    ]
}