{
    "version": 2,
    "builds": [
      {
        "src": "backend/manage.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      },
      {
        "src": "frontend",
        "use": "@vercel/static-build",
        "config": { "zeroConfig": true }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/frontend/static/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/backend/manage.py/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/backend/manage.py"
      }
    ]
  }
  